<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".AmerituNomalFragment">

    <ImageView
        android:id="@+id/NomalPhoto"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/nomal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/score"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:text="@string/score"
        android:textSize="32sp"
        app:layout_constraintEnd_toEndOf="@+id/NomalPhoto"
        app:layout_constraintStart_toStartOf="@+id/NomalPhoto"
        app:layout_constraintTop_toTopOf="@+id/NomalPhoto" />

</androidx.constraintlayout.widget.ConstraintLayout>